          MODULE=ethtool
         VERSION=3.4.1
          SOURCE=$MODULE-$VERSION.tar.xz
   SOURCE_URL[0]=$KERNEL_URL/pub/software/network/$MODULE
      SOURCE_VFY=sha1:764e2e7ffd50f7d0df10218bb22d48930219f1ff
        WEB_SITE=http://www.kernel.org/pub/software/network/ethtool
         ENTERED=20031026
         UPDATED=20120615
           SHORT="Ethernet diagnostic and tuning tool"

cat << EOF
Ethtool is a Linux net driver diagnostic and tuning tool for the
Linux series of kernels. It obtains information and diagnostics 
related to media, link status, driver version, PCI (or other) bus 
location, and more. Ethtool obsoletes mii-tool.
EOF
